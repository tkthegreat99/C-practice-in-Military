#include<stdio.h>
#pragma warning(disable: 4996)
#include<string.h>
int main() {
	char ch[20];
	char *p, *q;
	int len=0;
	
	p = ch;
	scanf("%s", ch);
	for (p = ch; p < ch + 20; p++) {
		len++;
		if (*p == NULL) {
			break;
		}
	}
	
	for (p = ch; p < ch + len; p++) {
		if (*p == '#') {
			
			for (q = p - 1; q >= ch; q--) {
				printf("%c", *q);
			}
    }
	}
}
